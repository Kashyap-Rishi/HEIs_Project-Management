// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Userrandom {

   id        Int      @id @default(autoincrement())
   email     String   @unique
   username  String   @unique
   password  String
   instututename  String
   degree    String
   branch    String
   startyear String
   endyear   String
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt
} 

model Instituterandom {
   id        Int      @id @default(autoincrement())
   email     String   @unique
   username  String   @unique
   password  String
   institutename  String
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt
}

model Project {
  id              Int      @id @default(autoincrement())
  projectName     String
  description     String
  studentName     String
  rollNo          String
  dateOfUploading DateTime
  datasets        String
  results         String
  methods         String
  status          String

  languages       String[] 

  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}







